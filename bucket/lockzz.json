{
    "version": "1.0.0",
    "description": "lockzz - Portable application from cube one",
    "homepage": "https://git.cubeone.de/marvin.fenssel/auto-updater",
    "license": "Proprietary",
    "url": "https://git.cubeone.de/marvin.fenssel/auto-updater/-/raw/main/additional_software/lockzz.zip?private_token=$env:GITLAB_PAT",
    "extract_dir": "lockzz",
    "bin": "lockzz.exe",
    "shortcuts": [
        [
            "lockzz.exe",
            "lockzz"
        ]
    ],
    "pre_install": [
        "if (-not $env:GITLAB_PAT) { throw 'GITLAB_PAT environment variable not set' }",
        "Write-Host 'Downloading lockzz with private token...' -ForegroundColor Blue"
    ],
    "post_install": [
        "Write-Host 'lockzz installed successfully!' -ForegroundColor Green"
    ]
}
